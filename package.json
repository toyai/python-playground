{
  "name": "python-playground",
  "version": "0.0.1",
  "scripts": {
    "build": "python scripts/gen_pycompletions.py && vite build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "fmt": "prettier --write . && sh scripts/run_code_style.sh fmt",
    "dev": "vite",
    "lint": "prettier --check . && sh scripts/run_code_style.sh lint",
    "release": "node scripts/release.js",
    "serve": "vite preview",
    "setup": "pnpm i && pip install -r api/requirements.txt && sh scripts/run_code_style.sh install",
    "setup:ci": "pnpm i --frozen-lockfile && pip install -r api/requirements.txt && sh scripts/run_code_style.sh install"
  },
  "devDependencies": {
    "@codemirror/autocomplete": "^0.18.5",
    "@codemirror/closebrackets": "^0.18.0",
    "@codemirror/commands": "^0.18.2",
    "@codemirror/comment": "^0.18.1",
    "@codemirror/fold": "^0.18.1",
    "@codemirror/gutter": "^0.18.3",
    "@codemirror/highlight": "^0.18.4",
    "@codemirror/history": "^0.18.1",
    "@codemirror/lang-python": "^0.18.0",
    "@codemirror/language": "^0.18.1",
    "@codemirror/matchbrackets": "^0.18.0",
    "@codemirror/search": "^0.18.4",
    "@codemirror/state": "^0.18.7",
    "@codemirror/view": "^0.18.13",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.11",
    "conventional-changelog-cli": "^2.1.1",
    "prettier": "^2.3.0",
    "prompts": "^2.4.1",
    "semver": "^7.3.5",
    "vite": "^2.3.4",
    "vite-plugin-windicss": "^0.16.0",
    "vue": "^3.0.11",
    "windicss": "^3.0.12"
  },
  "description": "The Python Playground",
  "keywords": [
    "python",
    "playground",
    "repl"
  ],
  "author": "Jeff Yang",
  "license": "MIT",
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "printWidth": 80,
    "trailingComma": "none"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
